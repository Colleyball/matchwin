page,.page {
  font-family: 'PingFang SC', 'Helvetica Neue' ,Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
    background: url('http://www.aibotiyu.com/ImgFiles/ABSports/matchwin/data/data.png') no-repeat;
    background-attachment: fixed;
    background-size: 100%;
    background-position: top center
}

.enter {
  position: fixed;
  bottom:0rpx;
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  background: rgba(255,255,255,0.8);
  box-shadow: 0rpx 10rpx 100rpx 0rpx rgba(0, 0, 0, 0.3);
}

.enter-buttom {
  width: 600rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 10rpx;
  background: #09BB07;
  color: white;
  text-align: center;
  font-size: 35rpx;
}