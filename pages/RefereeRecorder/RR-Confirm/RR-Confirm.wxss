page,.page {
  font-family: 'PingFang SC', 'Helvetica Neue' ,Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
  background:url('http://www.volleywang.cn/liansaiquan/images/matchbox/sb/bg.png') no-repeat;
  background-size: 100%;
  background-position: bottom;
  position: absolute;
  background-attachment: fixed;
}

.top{
  width: 680rpx;
  height: 150rpx;
  line-height: 150rpx;
  margin-left: auto;
  margin-right: auto;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.2);
  background: url('https://volleywang.cn/liansaiquan/images/main/title-bg.png') center no-repeat;
  background-size: contain;
  text-align: left;
  padding-left: 50rpx;
  font-size: 55rpx;
  margin-bottom: 30rpx;
  color: #1A1A1A
}

.cover{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(255,255,255,0.95);
  overflow: hidden;
  z-index: 9998;
}

.resultlist{
  margin-top: 20rpx;
}


.result{
  position: relative;
  width: 680rpx;
  height:250rpx;
  background-color: white;
  padding: 0;
  border-radius: 20rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 40rpx 35rpx;
  text-align: center;
}

.teamA{
  width: 100%;
  height: 125rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
}

.teamB{
  width: 100%;
  height: 125rpx;
  line-height: 165rpx;
}

.vs{
  width: 640rpx;
  height: 80rpx;
  background-color: #3369E8;
  border-radius: 60rpx;
  line-height:80rpx;
  margin-top: -165rpx;
  margin-left: auto;
  margin-right: auto;
  color: white;
  z-index: 9999;
  font-size: 35rpx;
}

.Ref{
  position: relative;
  width: 680rpx;
  height:auto;
  background-color: white;
  padding: 0;
  border-radius: 20rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 50rpx 35rpx;
  text-align: center;
  font-size: 35rpx;
}

.RefA{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
}

.RefB{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}

.submit{
 border: 2px solid rgba(0, 0, 0, 0.2);
 border-radius: 20rpx;
 margin-top: 20rpx;
 width:680rpx;
 height:100rpx;
 margin-left: 35rpx;
 line-height: 100rpx;
 background-color: rgba(255, 255, 255, 0.8);
 color: black
}