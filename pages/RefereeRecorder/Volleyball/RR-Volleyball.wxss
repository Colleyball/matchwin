.title{
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: #0FB6ED;
  text-align: center;
  height: 80rpx;
  font-size: 30rpx
}

.teamnameA{
  width: 45%;
  margin-left: 2.5%;
  border-radius: 10rpx;
  background-color: whitesmoke;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2)
}

.teamnameB{
  width: 45%;
  margin-left: 5%;
  border-radius: 10rpx;
  background-color: whitesmoke;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.2)
}

.court{
  width: 100%;
  height: auto;
  background-color: #0FB6ED;
  text-align: center;
  position: absolute
}

.matchinfo{
  position: relative;
  width: 100%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  margin: 15rpx 0 0 0;
  border: 0;
  padding: 0;
}

.playcourt{
  width: 750rpx;
  height: 375rpx;
  background-color: #F6F900;
  margin-left: auto;
  margin-right: auto;
  margin-top: 10rpx;
  position: relative;
  border: none;
  display: flex;
  flex-direction: column;
  box-shadow: 0 0 40px rgba(0, 0, 0, 0.3);
}

.control{
  position: relative;
  width: 100%;
  height: 200rpx;
  margin: 15rpx 0 20rpx 0;
  border: 0;
  padding: 0;
  display: flex;
  flex-direction: row;
  text-align: center;
}

.TeamAScore{
  margin-top: 10rpx;
  width: 180rpx;
  height: 180rpx;
  line-height: 180rpx;
  border-radius: 20rpx;
  margin-left: 85rpx;
  background-color: whitesmoke;
  font-size: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
  border: 1rpx solid rgba(0, 0, 0, 0.3)
}

.TeamBScore{
  margin-top: 10rpx;
  width: 180rpx;
  height: 180rpx;
  line-height: 180rpx;
  border-radius: 20rpx;
  margin-left: 10rpx;
  background-color: whitesmoke;
  font-size: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
  border: 1rpx solid rgba(0, 0, 0, 0.3)
}

.teaminfo{
  width: 200rpx;
  height: 180rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  display: flex;
  flex-direction: column;
}

.Timeout{
  height: 90rpx;
  width: 200rpx;
  display: flex;
  flex-direction: row;
}

.Substitution{
  height: 90rpx;
  width: 200rpx;
  display: flex;
  flex-direction: row;
}

.teaminfo .itemtitle{
  width: 80rpx;
  color: blue;
  font-size: 60rpx;
}

.lefttimes{
  width: 60rpx;
  color: white;
  font-size: 60rpx;
}

.player{
  display: flex;
  flex-direction: column;
  height: 125rpx;
  line-height: 125rpx;
  margin: 0;
  border: 0;
  padding: 0;
}

.player .num{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0;
  border: 0;
  padding: 0;
  font-size: 40rpx;
}

.player .name{
  width: 100%;
  height: 55rpx;
  line-height: 55rpx;
  margin: 0;
  border: 0;
  padding: 0;
  font-size: 25rpx;
}

.AddScoreA{
  margin-top: 10rpx;
  width: 265rpx;
  height: 180rpx;
  line-height: 180rpx;
  border-top-right-radius: 90rpx;
  border-bottom-right-radius: 90rpx;
  margin-left: 0rpx;
  background-color: whitesmoke;
  font-size: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
}

.AddScoreB{
  margin-top: 10rpx;
  width: 265rpx;
  height: 180rpx;
  line-height: 180rpx;
  border-top-left-radius: 90rpx;
  border-bottom-left-radius: 90rpx;
  margin-left: 10rpx;
  background-color: whitesmoke;
  font-size: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
}

.AddScorehover{
  background-color: rgba(0, 0, 0, 0.2);
}

.othercontrol{
  width: 200rpx;
  height: 180rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
  display: flex;
  flex-direction: column;
}

.Timeoutcontrol{
  margin-top: 5rpx;
  height: 80rpx;
  width: 200rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
  background-color: whitesmoke;
  color: black;
}

.Substitutioncontrol{
  margin-top: 10rpx;
  height: 80rpx;
  width: 200rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.3);
  background-color: whitesmoke;
  color: black;
}

.Score{
  position: absolute;
  margin-top: 850rpx;
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
  background-color: white;
}

.match{
  height: 100rpx;
  line-height: 100rpx;
  width: 20%;
  font-size: 50rpx;
}

.set{
  height: 100rpx;
  line-height: 100rpx;
  width: 16%;
  font-size: 30rpx;
}

.Scoretip{
  position: absolute;
  margin-top: 920rpx;
  height: 50rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
  background-color: white;
}

.matchtip{
  height: 50rpx;
  line-height: 50rpx;
  width: 20%;
  font-size: 25rpx;
}

.settip{
  height: 50rpx;
  line-height: 50rpx;
  width: 16%;
  font-size: 25rpx;
}

.tool{
  position: absolute;
  margin-top: 990rpx;
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  text-align: center;
}

.toolitems{
  width: 25%;
}

.toolitems2{
  width: 50%;
}


.toolcontent{
  width: 90%;
  margin-left: 5%;
  height: 100%;
  background-color: #f7ebf2;
  border-radius: 20rpx;
  line-height: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
}

.toolcontent2{
  width: 90%;
  margin-left: 5%;
  height: 100%;
  background-color: #f7ebf2;
  border-radius: 20rpx;
  line-height: 100rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
  background-image:-webkit-linear-gradient(to top, #8f1d78, rgba(255,255,255,0)); 
  background-image:linear-gradient(to top,#8f1d78,rgba(255,255,255,0));
  color: white;
}

.cover{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(255,255,255,0.95);
  overflow: hidden;
  z-index: 9998;
}

.scoremethod{
  position:fixed;
  display: flex;
  flex-direction: column;
  width: 300rpx;
  top: 580rpx;
  left: 225rpx;
  align-items: center;
  z-index: 9998;
}

.toollist{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  line-height: 200rpx;
  text-align: center;
  margin-top: 30rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
}

.spike{
  background-image:-webkit-linear-gradient(to bottom, #0060c0, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#0060c0,rgba(255,255,255,0));
}

.server{
  background-image:-webkit-linear-gradient(to bottom, #f1ff2d, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#f1ff2d,rgba(255,255,255,0));
}

.block{
  background-image:-webkit-linear-gradient(to bottom, #5ed5d1, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#5ed5d1,rgba(255,255,255,0));
}

.fault{
  background-image:-webkit-linear-gradient(to bottom, #eaf986, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#eaf986,rgba(255,255,255,0));
}

.cancel{
  width: 150rpx;
  height: 150rpx;
  position: fixed;
  bottom:-500rpx;
  left:300rpx;
  border-radius: 100rpx;
  line-height: 150rpx;
  text-align: center;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
  z-index: 9999;
}

.table{
  position: fixed;
  right: 75rpx;
  top:200rpx;
  width:600rpx;
  height: 600rpx;
  border: 1rpx solid black;
  display: flex;
  flex-direction: row;
  text-align: center;
  z-index: 9999;
  border-radius: 20rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
  font-size: 28rpx;
}

.front{
  width: 200rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
}

.before{
  width: 400rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
}

.a{
  width: 200rpx;
  height: 200rpx;
  line-height: 200rpx;
  background-color: rgba(34, 143, 189, 0.4)
}
.b{
  width: 400rpx;
  height: 200rpx;
  line-height: 200rpx;
}

.showtimeout{
  width: 500rpx;
  height: 200rpx;
  border-radius: 20rpx;
  line-height: 200rpx;
  text-align: center;
  margin-top: 60rpx;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.2);
}

.teamAtimeout{
  background-image:-webkit-linear-gradient(to bottom, #0060c0, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#0060c0,rgba(255,255,255,0));
}

.teamBtimeout{
  background-image:-webkit-linear-gradient(to bottom, #f1ff2d, rgba(255,255,255,0)); 
  background-image:linear-gradient(to bottom,#f1ff2d,rgba(255,255,255,0));
}

.timeouttime{
  position:fixed;
  display: flex;
  flex-direction: column;
  width: 300rpx;
  top: 540rpx;
  left: 225rpx;
  align-items: center;
  z-index: 9998;
  font-size: 550rpx;
  color: rgba(0, 0, 0, 0.4)
}
