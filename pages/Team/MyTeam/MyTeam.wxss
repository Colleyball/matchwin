.teamlist{
  margin-top: 0rpx;
}

.cover{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(255,255,255,0.95);
  overflow: hidden;
  z-index: 9998;
}

.team{
  position: relative;
  width: 680rpx;
  height:520rpx;
  background-color: white;
  padding: 0;
  border-radius: 20rpx;
  box-shadow: 0rpx 20rpx 70rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 70rpx 35rpx;
}

.teampic{
  width: 680rpx;
  height: 360rpx;
  margin:0;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.teamname{
  margin-top: 0;
  width: 90%;
  margin-left: 5%;
  height: 90rpx;
  text-align: left;
  font-size: 50rpx;
  line-height: 90rpx;
  color: rgba(0, 0, 0,0.7)
}

.teamdetail{
  width: 90%;
  margin-left: 5%;
  height: 60rpx;
  text-align: left;
  font-size: 30rpx;
  line-height: 60rpx;
  color: rgba(0, 0, 0,0.5)
}

.like{
  position: absolute;
  font-family: "微软雅黑";
  color: blueviolet;
  text-align: center;
  width: 250rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50rpx;
  border: 1px solid blueviolet;
  margin: -60rpx 400rpx;
  background-color: whitesmoke;
  font-size: 40rpx;
  z-index: 9997;
  box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.6);
}

.Top{
    height: 150rpx;
    background-image: url("http://123.206.177.183/liansaiquan/images/mylike/mylike.png");
    background-size: cover;
    background-repeat: no-repeat;
    text-align: center;
    line-height: 150rpx;
    color: white;
    font-size: 60rpx;
    margin-bottom: 20rpx;
    width: 100%;
}