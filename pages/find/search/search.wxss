.search{
  width: 100%;
  height: 140rpx;
  background-color: #3369E8;
  position: fixed;
  top:0;
  z-index: 9999;
  text-align: center;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.5);
}

.searcharea{
  height: 85rpx;
  line-height: 85rpx;
  width: 90%;
  display: flex;
  flex-direction: row;
  background-color: white;
  box-shadow: 0rpx 10rpx 20rpx rgba(0, 0, 0,0.4);
  margin-left: 5%;
  border-radius: 5rpx;
}

.searchbox{
  height: 100%;
  width: 80%;
  border: 0;
  border-radius: 5rpx 0 0 5rpx;
}

.btn{
  height: 100%;
  width: 20%;
  margin-left: 0rpx;
  border: 0;
}

.submit{
 height: 100%;
 width: 100%;
 border-radius: 0rpx 5rpx 5rpx 0rpx;
}

.hover{
  background-color: rgba(0, 0, 0,0.1)
}

.title{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 25rpx;
  margin-top: 10rpx;
  color: #fff
}

.resultlist{
  margin-top: 200rpx;
}

.resultnum{
  position: relative;
  width: 680rpx;
  height:50rpx;
  color: rgba(0, 0, 0,0.5);
  font-size: 30rpx;
  line-height: 50rpx;
  background-color: white;
  padding: 0;
  border-radius: 20rpx;
  box-shadow: 0rpx 0rpx 30rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 40rpx 35rpx;
  text-align: center;
}

.result{
  position: relative;
  width: 680rpx;
  height:250rpx;
  background-color: white;
  padding: 0;
  border-radius: 20rpx;
  box-shadow: 0rpx 10rpx 50rpx rgba(0, 0, 0,0.3);
  margin: 0rpx 0rpx 70rpx 35rpx;
  text-align: center;
}

.teamA{
  width: 100%;
  height: 125rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
}

.teamB{
  width: 100%;
  height: 125rpx;
  line-height: 165rpx;
}

.vs{
  width: 100rpx;
  height: 100rpx;
  background-color: #3369E8;
  border-radius: 60rpx;
  line-height:100rpx;
  margin-top: -175rpx;
  margin-left: auto;
  margin-right: auto;
  color: white;
  z-index: 9999;
}