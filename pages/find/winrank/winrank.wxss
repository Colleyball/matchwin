/* pages/find/winrank/winrank.wxss */
.top{
  width: 680rpx;
  height: 150rpx;
  line-height: 150rpx;
  margin-left: auto;
  margin-right: auto;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  box-shadow: 0px 0px 30px rgba(0, 0, 0, 0.2);
  background: url('https://volleywang.cn/liansaiquan/images/main/title-bg.png') center no-repeat;
  background-size: contain;
  text-align: left;
  padding-left: 50rpx;
  font-size: 55rpx;
  margin-bottom: 30rpx;
  color: #1A1A1A
}

.college{
  display: flex;
  width:680rpx;
  height:160rpx;
  margin-left: 35rpx;
  line-height: 160rpx;
  flex-direction: row;
  background-color: whitesmoke;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-bottom: 40rpx;
  box-shadow: 0px 10px 30px rgba(0, 0, 0, 0.2);
}

.rank{
  position: relative;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 50rpx;
  margin-left: 20rpx;
  margin-top: 40rpx;
  background-color: rgba(94, 213, 209, 0.8)
}

.collegeh2h{
  display: flex;
  flex-direction: column;
  position: relative;
  width: 450rpx;
  height: 160rpx;
  margin-left: 20rpx;
}

.name{
  position: relative;
  width: 450rpx;
  height: 90rpx;
  margin: 0rpx;
  line-height: 90rpx;
}

.detail{
  position: relative;
  width: 450rpx;
  height: 80rpx;
  margin: 0rpx;
  line-height: 80rpx;
  font-size:25rpx;
  color: rgba(0, 0, 0, 0.5)
}

.pre{
  position: relative;
  width: 120rpx;
  height: 160rpx;
  margin-left: 20rpx;
  font-size: 50rpx;
  font-style: italic;
  margin-left: -50rpx;
  color: rgba(0, 0, 0, 0.5)
}
